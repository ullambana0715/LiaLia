<?xml version="1.0" encoding="utf-8"?>
<com.duanqu.qupai.widget.TextOnlyEditOverlay
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:duanqu="http://schemas.android.com/apk/res-auto"
    android:id="@+id/edit_overlay"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <com.duanqu.qupai.widget.AutoScaleTextView
	    android:id="@android:id/content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"        
        android:background="?attr/qupaiDIYOverlayContent_frame"
        android:contentDescription="@null"
        android:duplicateParentState="true"		           
        android:gravity="center"
        android:minWidth="40dp"
        duanqu:strokeColor="#FFff0000"
       	duanqu:strokeJoinStyle="miter"
       	duanqu:strokeWidth="1dp"/>

    <View
        android:id="@+id/content_frame_indiator"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:background="?attr/qupaiDIYOverlayDotte_frame"
        android:visibility="gone"/>
    <!--<com.duanqu.qupai.widget.DottedFrame
        android:id="@+id/content_frame_indiator"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:visibility="gone"/>-->

    <ImageView
        android:id="@+id/btn_edit_overlay_cancel"
        android:layout_width="@dimen/qupai_effect_diyoverlay_operation_size"
        android:layout_height="@dimen/qupai_effect_diyoverlay_operation_size"
        android:contentDescription="@null"
        android:duplicateParentState="true"
        android:layout_gravity="left|top"
        android:padding="10dp"
        android:src="?attr/qupaiDIYOverlayCancel" />   

    <ImageView
        android:id="@+id/btn_edit_overlay_transform"
        android:layout_width="@dimen/qupai_effect_diyoverlay_operation_size"
        android:layout_height="@dimen/qupai_effect_diyoverlay_operation_size"
        android:contentDescription="@null"
        android:duplicateParentState="true"
        android:layout_gravity="right|bottom"
        android:padding="10dp"
        android:src="?attr/qupaiDIYOverlayTransform" />
    
    <ImageView 
        android:id="@+id/btn_edit_overlay_text"
        android:layout_width="@dimen/qupai_effect_diyoverlay_operation_size"
        android:layout_height="@dimen/qupai_effect_diyoverlay_operation_size"
        android:contentDescription="@null"
        android:duplicateParentState="true"
        android:layout_gravity="right|top"
        android:padding="10dp"
        android:src="?attr/qupaiDIYOverlayText"/>

    <ImageView
        android:id="@+id/btn_edit_overlay_mirror"
        android:layout_width="@dimen/qupai_effect_diyoverlay_operation_size"
        android:layout_height="@dimen/qupai_effect_diyoverlay_operation_size"
        android:contentDescription="@null"
        android:duplicateParentState="true"
        android:layout_gravity="left|bottom"
        android:padding="10dp"
        android:src="?attr/qupaiDIYOverlayMirror"/>

</com.duanqu.qupai.widget.TextOnlyEditOverlay><!-- From: file:/D:/android_studio_1_5/yopper_as_2.3.0/Yopper/VideoTrimActivity/src/main/res/layout/qupai_text_edit_overlay.xml --><!-- From: file:/D:/android_studio_1_5/yopper_as_2.3.0/Yopper/VideoTrimActivity/build/intermediates/bundles/debug/res/layout/qupai_text_edit_overlay.xml -->