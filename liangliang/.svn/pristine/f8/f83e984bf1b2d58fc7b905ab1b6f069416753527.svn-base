<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="54dp"
    android:background="@color/qupai_black_opacity_40pct"
    android:orientation="horizontal"
    android:gravity="center_vertical">

    <ImageView
        android:id="@+id/btn_dubbing"
        android:layout_width="44dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:scaleType="center"
        android:src="?attr/qupaiDubbingButton"
        android:visibility="visible" />

    <TextView
        android:id="@+id/txt_track_dubbing"
        android:layout_width="54dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="@string/qupai_audio_mix_track_dubbing"
        android:textColor="#57e957"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/txt_track_music"
        android:layout_width="54dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="@string/qupai_audio_mix_track_music"
        android:textColor="#57e957"
        android:textSize="15sp" />

    <SeekBar
        android:id="@+id/sb_audio_mix_weight"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:progressDrawable="@drawable/sb_progress_qupai_audio_mix_weight"
        android:max="1000" />

    <TextView
        android:layout_width="54dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="@string/qupai_audio_mix_track_main"
        android:textColor="#26b8e9"
        android:textSize="15sp" />

</LinearLayout>
